<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="IoTLib_Test.Views.UserControl_Pwm_Adc">
  <ScrollViewer>
    <StackPanel>
      <!-- PWM Test -->
      <TextBlock Text="PWM Test" Classes.ClassName="true"/>
      <TextBlock Name="txDescPwm" Classes.ClassDescription="true"/>
      <Grid ColumnDefinitions="*,*,*">
        <StackPanel Grid.Column="1">
          <TextBlock Text="PWM GPIO Pin:" Classes.ClassContentinfo="true"/>
          <TextBox Name="tbPwmPin"/>
        </StackPanel>
        <Button Name="btnPwm" Content="Toggle PWM"/>
      </Grid>
      <TextBlock Name="txInfoPwm" Classes.ClassInfo="true"/>
    </StackPanel>
  </ScrollViewer>
</UserControl>

<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="IoTLib_Test.Views.UserControl_Led">

  <ScrollViewer>
    <StackPanel>
      <!-- PWM Test - Value -->
      <TextBlock Text="LED Test" Classes.ClassName="true"/>
      <TextBlock Name="txDescLed" Classes.ClassDescription="true"/>
      <Grid ColumnDefinitions="*,*,*">
        <Button Name="btnLed" Content="Start LED Test"/>
      </Grid>
      <TextBlock Name="txInfoLed" Classes.ClassInfo="true"/>
    </StackPanel>
  </ScrollViewer>
</UserControl>
